{
    "manifest_version": 3,
    "name": "Type Flood Racer",
    "description": "My frist invovlement in a Hackathon project. This is a chrome extension helps the user mint transcations to NEON.",
    "version": "1.0",
    "permissions" : [
        "tabs",
        "activeTab",
        "scripting",
        "nativeMessaging"
    ],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker":"background.js",
      "type": "module"
      },
    "action":{
      "default_title": "Click me for something fun!"
    }
  }